@techreport{korbmacher_replication_2023,
  type = {Preprint},
  title = {The Replication Crisis Has Led to Positive Structural, Procedural, and Community Changes},
  author = {Korbmacher, Max and Azevedo, Flavio and Pennington, Charlotte Rebecca and Hartmann, Helena and Pownall, Madeleine and Schmidt, Kathleen and Elsherif, Mahmoud Medhat and Breznau, Nate and Robertson, Olly May and Kalandadze, Tamara and Yu, Shijun and Baker, Bradley James and O'Mahony, Aoife and Olsnes, Jørgen Ø.-S. and Shaw, John J and Gjoneska, Biljana and Yamada, Yuki and Röer, Jan Philipp and Murphy, Jennifer and Alzahawi, Shilaan and Grinschgl, Sandra and Oliveira, Catia Margarida and Wingen, Tobias and Yeung, Siu Kit and Liu, Meng and König, Laura M and {Albayrak-Aydemir}, Nihan and Lecuona, Oscar and Micheli, Leticia and Evans, Thomas Rhys},
  year = {2023},
  month = may,
  institution = {MetaArXiv},
  doi = {10.31222/osf.io/r6cvx},
  urldate = {2023-10-31},
  abstract = {The emergence of large-scale replication projects yielding successful rates substantially lower than expected caused the behavioural, cognitive, and social sciences to experience a so-called `replication crisis'. In this Perspective, we reframe this `crisis' through the lens of a credibility revolution, focusing on positive structural, procedural and community-driven changes. Second, we outline a path to expand ongoing advances and improvements. The credibility revolution has been an impetus to several substantive changes which will have a positive, long-term impact on our research environment.}
}

