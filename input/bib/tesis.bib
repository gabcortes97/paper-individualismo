@article{kolstad2009,
  title = {Self-Construal in Chile and Norway: Implications for Cultural Differences in Individualism and Collectivism},
  shorttitle = {Self-Construal in Chile and Norway},
  author = {Kolstad, Arnulf and Horpestad, Silje},
  year = {2009},
  month = mar,
  journal = {Journal of Cross-Cultural Psychology},
  volume = {40},
  number = {2},
  pages = {275--281},
  issn = {0022-0221, 1552-5422},
  doi = {10.1177/0022022108328917},
  url = {http://journals.sagepub.com/doi/10.1177/0022022108328917},
  urldate = {2023-06-28},
  abstract = {The study compares Chilean and Norwegian self-construal with regard to the concepts of independence and interdependence. Chile has been characterized as collectivistic and Norway as individualistic, and the hypothesis is that this characterization also distinguishes self-perception as independent and interdependent, respectively. The total sample consists of 368 university students. The instrument, Self-Construal Scale (SCS), measures the strength of independence and interdependence. The Chilean sample reports a higher score on both constructs. Both samples report the highest score on independence. The results are inconsistent with previous models that characterize Chile as collectivistic and Norway as individualistic. These characterizations are questioned. The findings also imply that a particular score on either independence or interdependence does not predict the score on the opposite variable. Individuals in the two cultures have a unique blend of independent and interdependent self-appraisal, which represent a composite mix of individualistic and collective elements in each culture.},
  langid = {english}
}

