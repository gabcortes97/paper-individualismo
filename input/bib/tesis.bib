@article{gayo2017,
  title = {El individuo frente a la sociedad o el western sociológico},
  author = {Gayo, Modesto},
  year = {2017},
  journal = {Estudios Públicos},
  number = {147},
  pages = {263--279},
  issn = {0718-3089},
  url = {https://www.estudiospublicos.cl/index.php/cep/article/view/108},
  urldate = {2024-09-16},
  abstract = {Unlike the Spanish transition, and even the Argentine transition, there is something uncomfortable about the political "transformation" that took place in Chile at the end of the 1980s. There remained in the air the sensation of having consummated an equivalent to the Catholic transubstantiation of wine into blood, of having produced a form of "gatopardismo", of having had to recognize that the substance of the Chilean political regime is the continuity of its "oligarchism"; pardoning, yes, all these attempts at lexical inventions. The Chilean transition to democracy brought in its baggage the joy of the end of the dictatorship, at least for half of its citizen-voters, together with the sadness resulting from a deep conviction of irremediable defeat. This implied that dictatorship and democracy were no longer the old enemies of the 1920s and 1930s, no longer the opposites that the liberals placed before the communist enemy.},
  langid = {spanish},
