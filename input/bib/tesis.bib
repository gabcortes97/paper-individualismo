@article{chiru2022,
  title = {Who Wants Technocrats? A Comparative Study of Citizen Attitudes in Nine Young and Consolidated Democracies},
  shorttitle = {Who Wants Technocrats?},
  author = {Chiru, Mihail and Enyedi, Zsolt},
  year = {2022},
  month = feb,
  journal = {The British Journal of Politics and International Relations},
  volume = {24},
  number = {1},
  pages = {95--112},
  issn = {1369-1481, 1467-856X},
  doi = {10.1177/13691481211018311},
  url = {http://journals.sagepub.com/doi/10.1177/13691481211018311},
  urldate = {2023-06-28},
  abstract = {Technocratic cabinets and expert, non-political ministers appointed in otherwise partisan cabinets have become a common reality in recent decades in young and older democracies, but we know little about how citizens see this change and what values, perceptions and experiences drive their attitudes towards technocratic government. The article explores the latter topic by drawing on recent comparative survey data from nine countries, both young and consolidated democracies from Europe and Latin America. Two individual-level characteristics trigger particularly strong support for the replacement of politicians with experts: low political efficacy and authoritarian values. They are complemented by a third, somewhat weaker factor: corruption perception. At the macro level, technocracy appeals to citizens of countries where the quality of democracy is deficient and where technocratic cabinets are a part of historical legacy. Surprisingly, civic activism and, partially, satisfaction with democracy enhance technocratic orientation, indicating such attitudes are not expressions of alienation or depoliticisation.},
  langid = {english},
  file = {/Users/gabrielcortes/Zotero/storage/PCX28DWC/Chiru y Enyedi - 2022 - Who wants technocrats A comparative study of citi.pdf}
}

