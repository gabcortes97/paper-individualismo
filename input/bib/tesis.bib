@article{deppisch2022,
  title = {How Rural Is Rural Populism? On the Spatial Understanding of Rurality for Analyses of Right‐wing Populist Election Success in Germany*},
  shorttitle = {How Rural Is Rural Populism?},
  author = {Deppisch, Larissa and Osigus, Torsten and Klärner, Andreas},
  year = {2022},
  month = jul,
  journal = {Rural Sociology},
  volume = {87},
  number = {S1},
  pages = {692--714},
  issn = {0036-0112, 1549-0831},
  doi = {10.1111/ruso.12397},
  url = {https://onlinelibrary.wiley.com/doi/10.1111/ruso.12397},
  urldate = {2023-11-14},
  abstract = {Abstract             In this article, the authors take up the thesis of the narrative that the support for right‐wing populist election successes is located in rural areas. For the case of the German right‐wing populist party Alternative für Deutschland (AfD) the authors propose a complex definition of rurality, and perform a quantitative small‐scale analysis of the national election results in Germany in 2017. They examine the potential connection between a high share of votes for the AfD and the rurality of a municipality. The results show that in eastern Germany, the fairly rural municipalities have comparatively high AfD vote shares, whereas in western Germany, the fairly rural and the non‐rural municipalities have similar AfD vote shares. Therefore, it appears that the thesis that rural areas are the source of the support of right‐wing populism applies to some, but not to all rural areas of Germany.},
  langid = {english}
}

