@article{carrascosalazar2021,
  title = {Women Who Choose Science: How Self-efficacy, Outcome Expectations, Barriers and Support Perceived Influence the Career Choice},
  shorttitle = {Women Who Choose Science},
  author = {Carrasco Salazar, Evelyn and Valenzuela Vidal, Daniela},
  year = {2021},
  month = jul,
  journal = {Calidad en la Educación},
  number = {54},
  pages = {271},
  issn = {0718-4565, 0717-4004},
  doi = {10.31619/caledu.n54.994},
  url = {https://www.calidadenlaeducacion.cl/index.php/rce/article/view/994},
  urldate = {2023-10-13},
  abstract = {The aim of this research from an exploratory-descriptive approach, is to reveal the characteristics of self-efficacy, outcome expectations, barriers and support perceived by women who are studying their first year of Science and Technology (STEM) careers at universities in the Metropolitan Region of Chile. Through semi-structured interviews and qualitative content analysis based on the Social Cognitive Career Theory (SCCT), it was possible to identify three factors related to their career choice: personal, agency and contextual elements, which act by fostering or blocking the process. Despite choosing STEM careers, female students have doubts related to gender stereotypes and socioemotional aspects such as anxiety and low tolerance to frustration. However, a positive self-efficacy and outcome expectations, family support and women´s student networks act as important drivers.},
  langid = {spanish},
  file = {/Users/gabrielcortes/Zotero/storage/6UEPIEWW/Carrasco Salazar y Valenzuela Vidal - 2021 - Women Who Choose Science How Self-efficacy, Outco.pdf}
}

